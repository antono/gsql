    	<placeholder name="PHolderFile"/>
    	<placeholder name="PHolderPrint" />
		<placeholder name="PHolderRecent" />
		<menuitem name="Quit" action="ActionExit" />
    </menu>
    <menu name="MenuEdit" action="ActionMenuEdit">
	   <placeholder name="PHolderEdit" />
	   <placeholder name="PHolderFormat" />
    </menu>
    <menu name="MenuSearch" action="ActionMenuSearch">
	   <placeholder name="PHolderSearch"/>
    </menu>
    <menu name="MenuView" action="ActionMenuView">
      <menuitem name="MenuShowNavarea" action="ActionShowNavarea"/>
      <menuitem name="MenuShowMessarea" action="ActionShowMessarea"/>
